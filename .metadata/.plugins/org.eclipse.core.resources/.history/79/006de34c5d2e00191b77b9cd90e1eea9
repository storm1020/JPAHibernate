package br.com.fiap.entity;

import java.util.Calendar;

import javax.persistence.Entity;

@Entity
@Table(nome= "T_CARGO") //Anotação não obrigatório.
public class Cargo {
	
	@Id
	private int codigo;
	
	private float salario;
	
	private Nivel nivel;
	
	private String descricao;
	
	private Calendar dataCadastro;
}
